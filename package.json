{
  "name": "@adiranids/vue3-tailwind",
  "version": "1.0.7",
  "license": "MIT",
  "description": "Vue 3 Styled Components Library using Tailwind CSS.",
  "files": [
    "dist"
  ],
  "main": "./dist/adiranidsvue3tailwind.umd.js",
  "module": "./dist/adiranidsvue3tailwind.es.js",
  "exports": {
    ".": {
      "import": "./dist/adiranidsvue3tailwind.es.js",
      "require": "./dist/adiranidsvue3tailwind.umd.js"
    },
    "./dist/style.css": {
      "import": "./dist/style.css",
      "require": "./dist/style.css"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "serve": "vite preview"
  },
  "dependencies": {
    "path": "^0.12.7",
    "rollup-plugin-postcss": "^4.0.2",
    "vue": "^3.2.16"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^1.9.3",
    "autoprefixer": "^10.4.0",
    "postcss": "^8.4.5",
    "tailwindcss": "^3.0.5",
    "typescript": "^4.4.3",
    "vite": "^2.6.4",
    "vue-tsc": "^0.3.0"
  }
}
